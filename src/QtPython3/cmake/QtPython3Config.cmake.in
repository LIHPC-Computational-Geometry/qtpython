find_dependency (Python3 REQUIRED COMPONENTS Development)

@PACKAGE_INIT@
set (QtPython3_VERSION @QT_Python_VERSION@)
set_and_check (QtPython3_INCLUDE_DIR   "@PACKAGE_INCLUDE_INSTALL_DIR@")
